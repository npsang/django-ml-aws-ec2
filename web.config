<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appSettings>
        <!-- Required settings-->
        <add key="WSGI_HANDLER" value="django.core.wsgi.get_wsgi_application()" />
        <!-- Your django path-->
        <add key="PYTHONPATH" value="C:\pyenv\djangoML\django-ml-aws-ec2\MLRestAPI" />
        <!-- Your djangoname.settings-->
        <add key="DJANGO_SETTINGS_MODULE" value="MLRestAPI.settings" />
    </appSettings>
    <system.webServer>
        <handlers>
            <add name="djangoiis2" path="*" verb="*" modules="FastCgiModule" scriptProcessor="C:\pyenv\djangoML\Scripts\python.exe|C:\pyenv\djangoML\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" />
            <add name="djangoiis" path="*" verb="*" modules="FastCgiModule" scriptProcessor="C:\pyenv\djangoML\Scripts\python.exe|C:\pyenv\djangoML\Lib\site-packages\wfastcgi.py" resourceType="File" />
        </handlers>
    </system.webServer>
